<application>
  <component name="LogHighlightingSettings">
    <Patterns>
      <list>
        <LogHighlightingPattern pattern="\s*e(rror)?\s*$" action="HIGHLIGHT_LINE" fg="-4258048" bold="true" stripe="true" />
        <LogHighlightingPattern pattern="\s*w(arn)?\s*$" action="HIGHLIGHT_LINE" fg="-22016" />
        <LogHighlightingPattern pattern="\s*i(nfo)?\s*$" action="HIGHLIGHT_LINE" fg="-12599489" />
        <LogHighlightingPattern pattern="\[err(or)?]" action="HIGHLIGHT_LINE" fg="-4258048" bold="true" stripe="true" />
        <LogHighlightingPattern pattern="\[warn(ing)?]" action="HIGHLIGHT_LINE" fg="-22016" />
        <LogHighlightingPattern pattern="\[info]" action="HIGHLIGHT_LINE" fg="-12599489" stripe="true" />
      </list>
    </Patterns>
    <parsing>
      <list>
        <LogParsingPattern name="Pipe-separated" pattern="^(?s)([^|]*)\|([^|]*)\|([^|]*)\|(.*)$" timePattern="HH:mm:ss.SSS" linePattern="^\d" timeId="0" severityId="1" categoryId="2" />
        <LogParsingPattern name="IntelliJ IDEA" pattern="^([^\[]+)(\[[\s\d]+])\s*(\w*)\s*-\s*(\S*)\s*-(.+)$" timePattern="yyyy-MM-dd HH:mm:ss,SSS" linePattern="^\d" timeId="0" severityId="2" categoryId="3" />
        <LogParsingPattern name="TeamCity build log" pattern="^\[([^]]+)](.):\s*(\[[^]]+])?(.*)$" timePattern="HH:mm:ss" linePattern="^\[" timeId="0" severityId="1" categoryId="2" />
        <LogParsingPattern name="Nginx error log" pattern="^([^\[]+) (\[[A-Za-z]+])\s+(\d+#\d+): (.+)$" timePattern="yyyy/MM/dd HH:mm:ss" linePattern="^\d" timeId="0" severityId="2" categoryId="3" />
      </list>
    </parsing>
    <settingsVersion>2</settingsVersion>
    <lastAddedDefaultFormat>3</lastAddedDefaultFormat>
  </component>
</application>