              <State>
                <id>Lua</id>
              </State>
              <State>
                <id>Method MetricsLua</id>
              </State>
              <State>
                <id>Performance issuesLua</id>
              </State>
              <State>
                <id>Probable bugsLua</id>
              </State>
              <State>
                <id>Validity issuesLua</id>
              </State>
            </expanded-state>
            <selected-state>
              <State>
                <id>Android</id>
              </State>
            </selected-state>
          </profile-state>
        </entry>
      </component>
      <component name="PropertiesComponent">
        <property name="settings.editor.selected.configurable" value="com.sylvanaar.idea.Lua.options.LuaOptionsPanel" />
